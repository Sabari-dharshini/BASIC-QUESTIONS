import java.util.Scanner;
class LCM{
public static void main(String[]args)
{
Scanner scan=new Scanner(System.in);
int a=scan.nextInt();
int b=scan.nextInt();
int originalA=a,originalB=b;

while(b!=0){
int temp=b;
b=a%b;
a=temp;

}
int gcd=a;
int lcm=(originalA*originalB/gcd);
System.out.println("LCM of two numbers : "+lcm);
}
}
